<Window x:Class="Film_geek.Windows.Overview" Style="{StaticResource DefaultWindow}"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Film_geek.Windows"
        xmlns:comm="clr-namespace:Film_geek.Classes"
        mc:Ignorable="d"
        Title="Overview" Height="600" Width="1040">
    <Window.CommandBindings>
        <CommandBinding Command="comm:AddFilmCommand.Add" Executed="AddFilm_Executed" CanExecute="AddFilm_CanExecute" />
        <CommandBinding Command="comm:LogOutCommand.LogOut" Executed="LogOut_Executed" CanExecute="LogOut_CanExecute" />
        <CommandBinding Command="comm:ProfileSettings.Profile" Executed="Profile_Executed" CanExecute="Profile_CanExecute" />
        <CommandBinding Command="comm:HelpManual.Help" Executed="Help_Executed" CanExecute="Help_CanExecute" />
        <CommandBinding Command="comm:FilmDetailCommand.Detail" Executed="Detail_Executed" CanExecute="Detail_CanExecute" />
    </Window.CommandBindings>
    
    <Window.Resources>
        <comm:ImageConverter x:Key="ImageConverter"/>
    </Window.Resources>
    <Grid Name="GD_Content">

    </Grid>
</Window>

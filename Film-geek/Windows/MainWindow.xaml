        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Film_geek.Windows"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">

    <StackPanel >
        <Button Click="sigin_Click" Padding="10" Margin="15" Name="sigin">SignInWindow</Button>
        <Button Click="create_account_Click" Padding="10" Margin="15" Name="create_account">create _account</Button>
        <Button Click="overview_Click" Padding="10" Margin="15" Name="overview">overview</Button>
        <Button Click="AOEFilm_Click" Padding="10" Margin="15" Name="AOEFilm">Add or Edit Film</Button>
        <Button Click="profil_Click" Padding="10" Margin="15" Name="profil">profil</Button>
    </StackPanel>

    
    <!-- Tylendotest-->
</Window>
